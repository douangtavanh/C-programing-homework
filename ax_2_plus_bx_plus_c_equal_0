#include <stdio.h>
#include <math.h>

int main()
{
    int a, b, c, delta, x;
    float x1, x2;
    printf("Please enter a value: \n");
    scanf("%d", &a);
    printf("Please entr b value: \n");
    scanf("%d", &b);
    printf("Please enter c value: \n");
    scanf("%d", &c);
    if (a == 0) {
        x = -c/b;
        printf("%d", x);
    } else {
        delta = pow(b, 2) - (4*a*c);
        if (delta == 0) {
            x = -b/(2*a);
            printf("%d", x);
        } else if (delta > 0) {
            x1 = (-b + sqrt(delta))/(2*a);
            x2 = (-b - sqrt(delta))/(2*a);
            printf("%f, %f", x1, x2);
        } else {
            printf("NOOO");
        }
            
    }
}